[user]
    email = howardad@gmail.com
    name = Adam Howard
[color]
    ui = auto
	diff = auto
	status = auto
	branch = auto
[push]
    default = simple
[core]
    autocrlf = input
    safecrlf = true
	excludesfile = /home/adam/.gitignore_global
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    type = cat-file -t
    dump = cat-file -p
